<div class="page-header">
<h1>Listing Posts</h1>
</div>

<table class="table table-striped">
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @posts.each do |post| %>
      <tr>  
        <td> <%=post.title%>
        </td>
        
        <td> <%=post.body%> </td>
        <td> <%=post.author%> </td>
         <td> <%=post.date%> </td>
        <td> <%=post.closing%> </td>
        <td> <%=post.email%> </td>
        <td> <%=post.test1%> </td>
        <td> <%=post.test2%> </td>
        <td> <%=post.test3%> </td>
        <td> <%=post.test4%> </td>

        <td><%= link_to 'Show', post %></td>
        <td><%= link_to 'Edit', edit_post_path(post) %></td>
        <td><%= link_to 'Destroy', post, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Post', new_post_path %>
